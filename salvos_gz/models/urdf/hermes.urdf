<?xml version="1.0"?>
<robot name="hermes">

<material name="blue">
    <color rgba="0 0 0.8 1"/>
  </material>
  <material name="black">
    <color rgba="0 0 0 1"/>
  </material>
  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>
  <material name="grey">
    <color rgba="0.8 0.8 0.8 1"/>
  </material>



  <link name="base_link">
  </link>


  <joint name="dummy_joint" type="fixed">
    <parent link="base_link"/>
    <child link="frame"/>
    <origin xyz="0 0 0.8" rpy="0 -1.571 0"/>
  </joint>


  <link name="frame">
    <visual>
      <origin rpy="0.0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://salvos_gz/models/hermes/meshes/hermes_mesh_scaled.stl"/>
      </geometry>
      <material name="grey"/>
    </visual>
    <collision>
      <origin rpy="0.0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://salvos_gz/models/hermes/meshes/hermes_mesh_scaled.stl"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>   <!-- -0.655083 -2.516692 1.106822 -->
      <mass value="1630.0"/>
    <inertia
      ixx="70.10" ixy="-0.003142" ixz="0.0001132"
      iyy="111.5" iyz="0.000095407"
      izz="111.5"/>
    </inertial>
  </link>


  <link name="nose_rotor">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://salvos_gz/models/hermes/meshes/propeller_scaled.stl"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin rpy="0.0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://salvos_gz/models/hermes/meshes/propeller_scaled.stl"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="5"/>
      <inertia
        ixx="43.00" ixy="-0.0486" ixz="-0.0486"
        iyy="34.33" iyz="0.000060945"
        izz="34.33"/>
    </inertial>
  </link>


  <joint name="nose_rotor_joint" type="continuous">
    <parent link="frame"/>
    <child link="nose_rotor"/>
    <origin xyz="1 0 0" rpy="0 1.571 3.14159"/>
    <axis xyz="0 0 1"/>
    <limit lower="0" upper="0" effort="0.1" velocity="100"/>
    <dynamics damping="0.01" friction="0.001"/>
  </joint>


  <link name="fl_rotor">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://salvos_gz/models/hermes/meshes/propeller_scaled.stl"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin rpy="0.0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://salvos_gz/models/hermes/meshes/propeller_scaled.stl"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="5"/>
      <inertia
        ixx="43.00" ixy="-0.0486" ixz="-0.0486"
        iyy="34.33" iyz="0.000060945"
        izz="34.33"/>
    </inertial>
  </link>


  <joint name="fl_rotor_joint" type="continuous">
    <parent link="frame"/>
    <child link="fl_rotor"/>
    <origin xyz="0.5 0.81 0.81" rpy="0 1.571 3.14159"/>
    <axis xyz="0 0 1"/>
    <limit lower="0" upper="0" effort="0.1" velocity="100"/>
    <dynamics damping="0.01" friction="0.001"/>
  </joint>



  <link name="fr_rotor">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://salvos_gz/models/hermes/meshes/propeller_scaled.stl"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin rpy="0.0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://salvos_gz/models/hermes/meshes/propeller_scaled.stl"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="5"/>
      <inertia
        ixx="43.00" ixy="-0.0486" ixz="-0.0486"
        iyy="34.33" iyz="0.000060945"
        izz="34.33"/>
    </inertial>
  </link>


  <joint name="fr_rotor_joint" type="continuous">
    <parent link="frame"/>
    <child link="fr_rotor"/>
    <origin xyz="0.5 -0.88 0.88" rpy="0 1.571 3.14159"/>
    <axis xyz="0 0 1"/>
    <limit lower="0" upper="0" effort="0.1" velocity="100"/>
    <dynamics damping="0.01" friction="0.001"/>
  </joint>



  <link name="rl_rotor">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://salvos_gz/models/hermes/meshes/propeller_scaled.stl"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin rpy="0.0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://salvos_gz/models/hermes/meshes/propeller_scaled.stl"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="5"/>
      <inertia
        ixx="43.00" ixy="-0.0486" ixz="-0.0486"
        iyy="34.33" iyz="0.000060945"
        izz="34.33"/>
    </inertial>
  </link>


  <joint name="rl_rotor_joint" type="continuous">
    <parent link="frame"/>
    <child link="rl_rotor"/>
    <origin xyz="0.5 -0.93 -0.93" rpy="0 1.571 3.14159"/>
    <axis xyz="0 0 1"/>
    <limit lower="0" upper="0" effort="0.1" velocity="100"/>
    <dynamics damping="0.01" friction="0.001"/>
  </joint>




  <link name="rr_rotor">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://salvos_gz/models/hermes/meshes/propeller_scaled.stl"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <collision>
      <origin rpy="0.0 0 0" xyz="0 0 0"/>
      <geometry>
        <mesh filename="package://salvos_gz/models/hermes/meshes/propeller_scaled.stl"/>
      </geometry>
    </collision>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="5"/>
      <inertia
        ixx="43.00" ixy="-0.0486" ixz="-0.0486"
        iyy="34.33" iyz="0.000060945"
        izz="34.33"/>
    </inertial>
  </link>


  <joint name="rr_rotor_joint" type="continuous">
    <parent link="frame"/>
    <child link="rr_rotor"/>
    <origin xyz="0.5 0.93 -0.93" rpy="0 1.571 3.14159"/>
    <axis xyz="0 0 1"/>
    <limit lower="0" upper="0" effort="0.1" velocity="100"/>
    <dynamics damping="0.01" friction="0.001"/>
  </joint>


  <gazebo>
    <plugin name="gz::sim::systems::JointController" filename="libgz-sim-joint-controller-system.so">
      <joint_name>nose_rotor_joint</joint_name>
      <topic>/model/hermes/joint/nose_rotor_joint/cmd_vel</topic>
    </plugin>
  </gazebo>


  <gazebo>
    <plugin name="gz::sim::systems::JointController" filename="libgz-sim-joint-controller-system.so">
      <joint_name>fl_rotor_joint</joint_name>
      <topic>/model/hermes/joint/fl_rotor_joint/cmd_vel</topic>
    </plugin>
  </gazebo>


  <gazebo>
    <plugin name="gz::sim::systems::JointController" filename="libgz-sim-joint-controller-system.so">
      <joint_name>fr_rotor_joint</joint_name>
      <topic>/model/hermes/joint/fr_rotor_joint/cmd_vel</topic>
    </plugin>
  </gazebo>

  <gazebo>
    <plugin name="gz::sim::systems::JointController" filename="libgz-sim-joint-controller-system.so">
      <joint_name>rl_rotor_joint</joint_name>
      <topic>/model/hermes/joint/rl_rotor_joint/cmd_vel</topic>
    </plugin>
  </gazebo>

  <gazebo>
    <plugin name="gz::sim::systems::JointController" filename="libgz-sim-joint-controller-system.so">
      <joint_name>rr_rotor_joint</joint_name>
      <topic>/model/hermes/joint/rr_rotor_joint/cmd_vel</topic>
    </plugin>
  </gazebo>


</robot>